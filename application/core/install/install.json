                "  `is_oauth2` tinyint(1) unsigned NOT NULL DEFAULT '0' COMMENT '是否OAuth2登录',",
                "  `create_time` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '创建时间',",
                "  `delete_time` int(11) NOT NULL DEFAULT '0' COMMENT '删除时间',",
